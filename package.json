{
  "name": "beestripes-website",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "node server",
    "build": "yarn build:client && yarn build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.jsx --outDir dist/server",
    "generate": "NODE_ENV=production vite build --outDir dist/static && yarn build:server && node prerender",
    "debug": "node --inspect-brk server",
    "deploy": "sh deploy.sh",
    "start": "yarn build && yarn dev",
    "start:static": "NODE_ENV=production yarn generate && serve dist/static"
  },
  "dependencies": {
    "autoprefixer": "^10.2.5",
    "framer-motion": "^4",
    "postcss": "^8.2.9",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "repaintless": "^1.4.0",
    "tailwindcss": "^2.1.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "serve": "^11.3.2",
    "serve-static": "^1.14.1",
    "vite": "^2.1.5"
  }
}
